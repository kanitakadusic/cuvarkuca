<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="spinner_focus_TEXT">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="plant_name_TEXT">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textColor">?attr/colorOnBackground</item>
        <item name="android:textSize">16sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="plant_info_TEXT">
        <item name="android:fontFamily">@font/roboto_light</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
        <item name="android:textSize">12sp</item>
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="plant_item_TEXT">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="android:textSize">14sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="ShapeAppearance.App.Fab" parent="ShapeAppearance.MaterialComponents">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>